<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="250dp"
    android:layout_height="wrap_content"
    android:background="#b5b5b5">


    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_margin="15dp"
        android:fontFamily="@font/trade_winds"
        android:text="Fonts"
        android:textColor="#ffffff"
        android:textSize="25sp"
        />

    <TextView
        android:id="@+id/tv_courgette"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView10"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="15dp"
        android:textSize="15dp"
        android:textColor="#ffffff"
        android:fontFamily="@font/courgette"
        android:text="Courgette" />

    <TextView
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="15dp"
        android:id="@+id/tv_roboto_italic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_courgette"
        android:fontFamily="@font/roboto_italic"
        android:text="Roboto_italic" />

    <TextView
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="15dp"
        android:id="@+id/tv_arizonia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_roboto_italic"
        android:fontFamily="@font/arizonia"
        android:text="Arizonia" />

    <TextView
        android:textStyle="bold"
        android:textColor="#ffffff"
        android:textSize="17dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="15dp"
        android:id="@+id/tv_dynalight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_arizonia"
        android:fontFamily="@font/dynalight"
        android:text="Dynalight" />

    <TextView
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="15dp"
        android:id="@+id/tv_galada"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_dynalight"
        android:fontFamily="@font/galada"
        android:text="Galada" />

    <TextView
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="15dp"
        android:id="@+id/tv_great_vibes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_galada"
        android:fontFamily="@font/great_vibes"
        android:text="Great_Vibes" />

    <TextView
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="15dp"
        android:id="@+id/tv_new_rocker"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_great_vibes"
        android:fontFamily="@font/new_rocker"
        android:text="New_Rocker" />

    <TextView
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="15dp"
        android:id="@+id/tv_oleo_script"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_new_rocker"
        android:fontFamily="@font/oleo_script"
        android:text="Oleo_Script" />

    <TextView
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="15dp"
        android:id="@+id/tv_sail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_oleo_script"
        android:fontFamily="@font/sail"
        android:text="Sail" />

    <TextView
        android:textColor="#ffffff"
        android:textSize="15dp"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="15dp"
        android:id="@+id/tv_tauri"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_sail"
        android:fontFamily="@font/tauri"
        android:text="Tauri"
        android:layout_marginBottom="15dp"/>
</RelativeLayout>